#include <iostream>
using namespace std;

class Point{
    private:
    int x;
    int y;
    public:

    Point(int xv , int yv){
        this->x = xv;
        this->y = yv; 
    }
    point(const Point& po){
        Point a = po;
    }
    void Print()const{
        cout << x << " " << y << endl;
    }

}


int main(){
    Point a(4,8);
    point b(a);

    return 0;
}